Proceso nombre_algoritmo
	Escribir "Hola mundo";
	Definir nombre como cadena;
	Definir edad Como Entero;
	Definir estatura Como Real;
	Escribir "Ingresa Nombre";
	Leer  nombre;
	Escribir "Ingresa Edad";
	Leer edad;
	Escribir "Ingrese su estatura";
	Leer estatura;
	Escribir "Hola ", nombre;
	Escribir "Tienes ", edad, " años";
	Escribir "Tu estatura es ",estatura;
FinProceso
